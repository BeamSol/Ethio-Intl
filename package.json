{
  "name": "ethio-intl",
  "version": "1.0.0",
  "description": "A JavaScript/React SDK for Ethiopian web applications with bilingual support, transliteration, and utility functions",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "test": "jest",
    "lint": "eslint src --ext .ts,.tsx",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "ethiopian",
    "internationalization",
    "i18n",
    "amharic",
    "oromo",
    "transliteration",
    "ethiopic",
    "react",
    "javascript"
  ],
  "author": "Ethio-Intl Team",
  "license": "MIT",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/ethio-intl.git"
  },
  "bugs": {
    "url": "https://github.com/your-org/ethio-intl/issues"
  },
  "homepage": "https://github.com/your-org/ethio-intl#readme",
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^15.0.0",
    "@rollup/plugin-typescript": "^11.0.0",
    "@types/jest": "^29.0.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "jest": "^29.0.0",
    "rollup": "^4.0.0",
    "rollup-plugin-dts": "^6.0.0",
    "ts-jest": "^29.0.0",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "i18next": "^25.7.2",
    "react-i18next": "^16.5.0",
    "tslib": "^2.8.1"
  }
}
